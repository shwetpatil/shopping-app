# Inventory Service Environment Variables

# Server
PORT=3006
NODE_ENV=development
SERVICE_NAME=inventory-service

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5436/inventory_db?schema=public

# Kafka
KAFKA_BROKERS=localhost:9093
KAFKA_CLIENT_ID=inventory-service
KAFKA_GROUP_ID=inventory-service-group

# Reservation Settings
RESERVATION_TIMEOUT=900

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Logging
LOG_LEVEL=info
